{
  "id": null,
  "title": "air-Q Luftqualität Dashboard",
  "tags": ["airq", "luftqualität", "sensoren"],
  "style": "dark",
  "timezone": "browser",
  "editable": true,
  "graphTooltip": 1,
  "time": {
    "from": "now-6h",
    "to": "now"
  },
  "timepicker": {
    "refresh_intervals": ["5s", "10s", "30s", "1m", "5m", "15m", "30m", "1h", "2h", "1d"],
    "time_options": ["5m", "15m", "1h", "6h", "12h", "24h", "2d", "7d", "30d"]
  },
  "refresh": "30s",
  "panels": [
    {
      "id": 1,
      "title": "Temperatur",
      "type": "stat",
      "targets": [
        {
          "expr": "airq_temperature_celsius",
          "legendFormat": "{{sensor_path}}"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "celsius",
          "min": 0,
          "max": 40,
          "thresholds": {
            "steps": [
              {"color": "blue", "value": null},
              {"color": "green", "value": 18},
              {"color": "yellow", "value": 25},
              {"color": "red", "value": 30}
            ]
          }
        }
      },
      "gridPos": {"h": 8, "w": 6, "x": 0, "y": 0},
      "options": {
        "colorMode": "background",
        "graphMode": "area",
        "justifyMode": "auto",
        "orientation": "auto"
      }
    },
    {
      "id": 2,
      "title": "Luftfeuchtigkeit",
      "type": "stat",
      "targets": [
        {
          "expr": "airq_humidity_percent",
          "legendFormat": "{{sensor_path}}"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "percent",
          "min": 0,
          "max": 100,
          "thresholds": {
            "steps": [
              {"color": "red", "value": null},
              {"color": "yellow", "value": 30},
              {"color": "green", "value": 40},
              {"color": "yellow", "value": 70},
              {"color": "red", "value": 80}
            ]
          }
        }
      },
      "gridPos": {"h": 8, "w": 6, "x": 6, "y": 0},
      "options": {
        "colorMode": "background",
        "graphMode": "area",
        "justifyMode": "auto",
        "orientation": "auto"
      }
    },
    {
      "id": 3,
      "title": "CO2-Konzentration",
      "type": "stat",
      "targets": [
        {
          "expr": "airq_co2_ppm",
          "legendFormat": "{{sensor_path}}"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "ppm",
          "min": 0,
          "max": 3000,
          "thresholds": {
            "steps": [
              {"color": "green", "value": null},
              {"color": "yellow", "value": 800},
              {"color": "orange", "value": 1200},
              {"color": "red", "value": 1600}
            ]
          }
        }
      },
      "gridPos": {"h": 8, "w": 6, "x": 12, "y": 0},
      "options": {
        "colorMode": "background",
        "graphMode": "area",
        "justifyMode": "auto",
        "orientation": "auto"
      }
    },
    {
      "id": 4,
      "title": "Luftdruck",
      "type": "stat",
      "targets": [
        {
          "expr": "airq_pressure_hpa",
          "legendFormat": "{{sensor_path}}"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "pressurehpa",
          "min": 950,
          "max": 1050,
          "thresholds": {
            "steps": [
              {"color": "blue", "value": null},
              {"color": "green", "value": 1000},
              {"color": "yellow", "value": 1030}
            ]
          }
        }
      },
      "gridPos": {"h": 8, "w": 6, "x": 18, "y": 0},
      "options": {
        "colorMode": "background",
        "graphMode": "area",
        "justifyMode": "auto",
        "orientation": "auto"
      }
    },
    {
      "id": 5,
      "title": "Temperatur Verlauf",
      "type": "timeseries",
      "targets": [
        {
          "expr": "airq_temperature_celsius",
          "legendFormat": "{{sensor_path}}"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "celsius",
          "custom": {
            "drawStyle": "line",
            "lineInterpolation": "linear",
            "lineWidth": 2,
            "fillOpacity": 10,
            "gradientMode": "none",
            "spanNulls": false,
            "pointSize": 5,
            "stacking": {"mode": "none", "group": "A"},
            "axisPlacement": "auto",
            "axisLabel": "",
            "scaleDistribution": {"type": "linear"},
            "hideFrom": {"legend": false, "tooltip": false, "vis": false}
          }
        }
      },
      "gridPos": {"h": 8, "w": 12, "x": 0, "y": 8}
    },
    {
      "id": 6,
      "title": "CO2 Verlauf",
      "type": "timeseries",
      "targets": [
        {
          "expr": "airq_co2_ppm",
          "legendFormat": "{{sensor_path}}"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "ppm",
          "custom": {
            "drawStyle": "line",
            "lineInterpolation": "linear",
            "lineWidth": 2,
            "fillOpacity": 10,
            "gradientMode": "none",
            "spanNulls": false,
            "pointSize": 5,
            "stacking": {"mode": "none", "group": "A"},
            "axisPlacement": "auto",
            "axisLabel": "",
            "scaleDistribution": {"type": "linear"},
            "hideFrom": {"legend": false, "tooltip": false, "vis": false}
          }
        }
      },
      "gridPos": {"h": 8, "w": 12, "x": 12, "y": 8}
    },
    {
      "id": 7,
      "title": "Feinstaub PM2.5",
      "type": "timeseries",
      "targets": [
        {
          "expr": "airq_pm25_ugm3",
          "legendFormat": "{{sensor_path}}"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "µg/m³",
          "custom": {
            "drawStyle": "line",
            "lineInterpolation": "linear",
            "lineWidth": 2,
            "fillOpacity": 10,
            "gradientMode": "none",
            "spanNulls": false,
            "pointSize": 5,
            "stacking": {"mode": "none", "group": "A"},
            "axisPlacement": "auto",
            "axisLabel": "",
            "scaleDistribution": {"type": "linear"},
            "hideFrom": {"legend": false, "tooltip": false, "vis": false}
          }
        }
      },
      "gridPos": {"h": 8, "w": 12, "x": 0, "y": 16}
    },
    {
      "id": 8,
      "title": "TVOC",
      "type": "timeseries",
      "targets": [
        {
          "expr": "airq_tvoc_ppb",
          "legendFormat": "{{sensor_path}}"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "ppb",
          "custom": {
            "drawStyle": "line",
            "lineInterpolation": "linear",
            "lineWidth": 2,
            "fillOpacity": 10,
            "gradientMode": "none",
            "spanNulls": false,
            "pointSize": 5,
            "stacking": {"mode": "none", "group": "A"},
            "axisPlacement": "auto",
            "axisLabel": "",
            "scaleDistribution": {"type": "linear"},
            "hideFrom": {"legend": false, "tooltip": false, "vis": false}
          }
        }
      },
      "gridPos": {"h": 8, "w": 12, "x": 12, "y": 16}
    },
    {
      "id": 9,
      "title": "Datenverarbeitung Status",
      "type": "stat",
      "targets": [
        {
          "expr": "rate(airq_sensor_data_total[5m])",
          "legendFormat": "{{sensor_path}} Datensätze/min"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "reqps",
          "decimals": 2,
          "thresholds": {
            "steps": [
              {"color": "red", "value": null},
              {"color": "yellow", "value": 0.01},
              {"color": "green", "value": 0.05}
            ]
          }
        }
      },
      "gridPos": {"h": 4, "w": 24, "x": 0, "y": 24},
      "options": {
        "colorMode": "background",
        "graphMode": "area",
        "justifyMode": "auto",
        "orientation": "horizontal"
      }
    }
  ]
} 